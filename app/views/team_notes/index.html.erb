<p id="notice"><%= notice %></p>

<h1>Listing Team Notes</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @team_notes.each do |team_note| %>
      <tr>
        <td><%= team_note.team_id %></td>
        <td><%= team_note.note_id %></td>
        <td><%= link_to 'Show', team_note %></td>
        <td><%= link_to 'Edit', edit_team_note_path(team_note) %></td>
        <td><%= link_to 'Destroy', team_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team note', new_team_note_path %>
